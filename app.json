{
  "expo": {
    "name": "expo-supported-locales",
    "slug": "expo-supported-locales",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "exposupportedlocales",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alextoudic.expo-supported-locales",
      "icon": {
        "tinted": "./assets/images/tinted-icon.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.alextoudic.exposupportedlocales"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-localization",
        {
          // Keep activity alive on locale change to avoid duplicate linking
          "allowDynamicLocaleChangesAndroid": true,
          "supportedLocales": [
            "en",
            "fr",
            "es"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
